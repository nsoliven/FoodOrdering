{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "paths": {
      "@components/*": [
        "./src/components/*"
      ],
      "@assets/*": [
        "./assets/*"
      ],
      "@constants/*": [
        "./src/constants/*"
      ],
      "@providers/*": [
        "./src/providers/*"
      ],
      "@lib/*": [
        "./src/lib/*"
      ],
      "@api/*": [
        "./src/api/*"
      ],
      "@src/*": [
        "./src/*"
      ]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts"
  ]
}
